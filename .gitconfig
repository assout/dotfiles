[include]
	path = .gitconfig.local
[user]
	name=assout
	email = assout@users.noreply.github.com
[push]
	default = simple
# for gist.vim
[github]
	user = assout
[core]
	excludesfile = ~/.gitignore
	quotepath = false
[alias]
	st = status
	cm = commit -m
	cma = commit -a -m
	br = branch
	co = checkout
	df = diff
	dfc = diff --cached
	dfh = diff HEAD
	lg = log -p

