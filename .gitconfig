[init]
  templatedir = ~/.git_templates/
[push]
  default = simple
[core]
  excludesfile = ~/.gitignore
  quotepath = false
  attributesfile = ~/.gitattributes
[alias]
  graph  = log --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  n = now --all --stat
[color]
  ui = true # 古いバージョンの場合defaultでないため
[include]
  path = .gitconfig.environment
[hub]
  protocol = https
[user "git@github.com"] # Refs: <http://klabgames.tech.blog.jp.klab.com/archives/1033121546.html>
  name = assout
  email = assout@users.noreply.github.com
[user "https://github.com"] # Refs: <http://klabgames.tech.blog.jp.klab.com/archives/1033121546.html>
  name = assout
  email = assout@users.noreply.github.com
[pre-commit]
  checks = [merge_conflict]

